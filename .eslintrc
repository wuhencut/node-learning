{
  "env": {
     "node": true,
     "es6": true
  },
  "extends": [
     "eslint:recommended",
     "plugin:node/recommended"
  ],
  "parserOptions": {
     "ecmaVersion": 2021
  },
  "rules": {
     "no-console": "off",
     "no-unused-vars": "warn",
     "node/no-unsupported-features/es-syntax": "off"
  },
  "plugins": [
     "node"
  ]
 }
 